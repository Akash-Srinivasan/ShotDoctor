{
  "expo": {
    "name": "FormCheck",
    "slug": "formcheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.formcheck.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.formcheck.app"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FormCheck to record your basketball shots"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow FormCheck to save your shot videos"
        }
      ]
    ],
    "scheme": "formcheck"
  }
}